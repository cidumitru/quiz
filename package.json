    "api:dev": "nx serve api",
    "api:start": "node dist/api/main.js",
    "start:prod": "node dist/api/main.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:db": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:db:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d"
